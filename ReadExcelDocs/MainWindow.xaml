<Window x:Class="ReadExcelDocs.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ReadExcelDocs"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
  <Grid>
    <Button x:Name='button'
            Content='Create Excel File'
            HorizontalAlignment='Left'
            VerticalAlignment='Top'
            Click='btnBrowes_Click' />

    <Button x:Name='excelbtn'
            Content='Open Excel File'
            HorizontalAlignment='Left'
            VerticalAlignment='Top'
            Margin='5.163,24.96,0,0'
            Click='excelbtn_Click' />

  </Grid>
</Window>
